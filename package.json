    "cap:run:android": "npx cap run android",
    "ios:build": "ionic build && npx cap sync ios",
    "android:build": "ionic build && npx cap sync android",
    "ios:dev": "ionic build && npx cap sync ios && npx cap open ios",
    "android:dev": "ionic build && npx cap sync android && npx cap open android",
    "dev:web": "ionic serve --lab",
    "dev:external": "ionic serve --external",
    "build:prod": "ionic build --prod",
    "deploy:prep": "npm run build:prod && npx cap sync"
